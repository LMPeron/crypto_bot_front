{
  "name": "servicedesk-project",
  "version": "0.0.7",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "serve": "ng serve",
    "build": "ng build",
    "build:prod": "ng build  --prod --outputHashing=all --aot=true --buildOptimizer=true ",
    "build:poc": "ng build --configuration=poc --outputHashing=all --aot=true --buildOptimizer=true ",
    "build:qa": "ng build --configuration=qa --outputHashing=all --aot=true --buildOptimizer=true ",
    "build:stats": "ng build --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "lint:auto": "ng lint --fix=true && npm run lint:styles",
    "lint:styles": "stylelint \"**/*.scss\"",
    "e2e": "ng e2e",
    "bundle-report": "webpack-bundle-analyzer dist/servicedesk-project/stats-es2015.json",
    "http-server": "http-server -p 8081 -c-1 dist/servicedesk-project",
    "heroku-postbuild": "ng build --configuration=qa --outputHashing=all",
    "prebuild": "npm --no-git-tag-version version patch",
    "prebuild:prod": "npm --no-git-tag-version version patch",
    "prebuild:poc": "npm --no-git-tag-version version patch",
    "prebuild:qa": "npm --no-git-tag-version version patch",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint",
    "pretty-quick": "pretty-quick"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.0.5",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "^11.0.5",
